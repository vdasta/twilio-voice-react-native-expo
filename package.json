{
  "name": "@simplevox/twilio-voice-expo",
  "version": "1.7.0-svox.1",
  "description": "Twilio Voice React Native SDK for SimpleVox",
  "main": "./cjs-entry.cjs",
  "react-native": "src/index",
  "exports": {
    ".": {
      "react-native": "./lib/module/index.js",
      "require": "./cjs-entry.cjs",
      "default": "./lib/module/index.js"
    },
    "./plugin/withTwilioVoiceIos": "./plugin/withTwilioVoiceIos.cjs"
  },
  "expo": {
    "plugin": "./plugin/withTwilioVoiceIos.cjs"
  },
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "files": [
    "plugin",
    "cjs-entry.cjs",
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "twilio-voice-react-native.podspec",
    "!lib/typescript/test/app",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "app": "npm run check && npm --prefix test/app install",
    "bootstrap": "npm run build:constants && npm --prefix test/app install && npm install && npm run pods",
    "build:api:extractor": "api-extractor run --local",
    "build:api:documenter": "api-documenter markdown --input-folder ./temp --output-folder ./docs/api",
    "build:constants": "node ./scripts/generate-constants.js",
    "build:docs": "npm run prepare && npm run build:api:extractor && npm run build:api:documenter",
    "build:errors": "node ./scripts/generate-errors.js",
    "check:lint": "eslint \"**/*.{js,ts,tsx}\"",
    "check:test": "jest",
    "check:type": "tsc --noEmit",
    "check:api": "git diff --exit-code src/error/generated.ts api/",
    "check": "npm run build:constants && npm run build:errors && npm run build:docs && npm run check:api && npm run check:type && npm run check:lint && npm run check:test --coverage",
    "pods": "cd test/app && pod-install --quiet",
    "prepare": "npm run build:constants && bob build",
    "release": "release",
    "test": "jest"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/vdasta/twilio-voice-react-native-expo.git",
  "author": "Vince Dasta (https://github.com/vdasta)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vdasta/twilio-voice-react-native-expo/issues"
  },
  "homepage": "https://github.com/vdasta/twilio-voice-react-native-expo#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@microsoft/api-extractor": "^7.25.2",
    "@ngrok/ngrok": "^1.3.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "18.2.48",
    "@types/react-native": "0.62.13",
    "api-documenter": "github:twilio/api-documenter#v0.0.1",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.19.2",
    "jest": "^28.1.3",
    "metro-react-native-babel-preset": "0.76.8",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.72.9",
    "react-native-builder-bob": "^0.18.3",
    "twilio": "^4.21.0",
    "twilio-release-tool": "^1.0.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/test",
      "<rootDir>/lib",
      "<rootDir>/types"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "test/app/",
    "src/error/generated.ts",
    "coverage/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@twilio/voice-errors": "1.8.0",
    "eventemitter3": "^4.0.7"
  }
}
